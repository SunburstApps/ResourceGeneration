<Project>
  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyTitle Condition="'$(AssemblyTitle)' == '' and '$(TargetName)' != ''">$(TargetName)</AssemblyTitle>
    <Description Condition="'$(Description)' == '' and '$(AssemblyTitle)' != ''">$(AssemblyTitle)</Description>

    <Authors>William Kent</Authors>
    <Copyright>Copyright Â© 2018-2019 William Kent. All rights reserved.</Copyright>
    <Product>Sunburst Windows Forms Common GUI Libraries</Product>

    <AssemblyVersion Condition="'$(AssemblyVersion)' == ''">2.1.0</AssemblyVersion>
    <FileVersion Condition="'$(FileVersion)' == '' and '$(Version)' != ''">$(Version)</FileVersion>
  </PropertyGroup>
</Project>
